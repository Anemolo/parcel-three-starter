{
  "name": "parcel-three-starter",
  "version": "1.0.0",
  "description": "A simple starter for my three projects",
  "main": "index.html",
  "staticFiles": {
    "staticPath": "static"
  },
  "targets": {
    "default": {}
  },
  "scripts": {
    "start": "parcel index.html",
    "build": "npm run clean && parcel build index.html --target default --no-cache",
    "deploy": "vercel deploy",
    "clean": "rm -rf dist/*.*"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "normalize.css": "^8.0.1",
    "three": "^0.125.2"
  },
  "devDependencies": {
    "@parcel/config-default": "2.0.0-beta.1",
    "parcel": "^2.0.0-beta.1",
    "parcel-reporter-static-files-copy": "^1.2.1"
  }
}
